{
    "name": "@safari-lib/fastify-boilerplate",
    "author": "BenoitSafari",
    "description": "Fastify boilerplate using, Fastify, Postgres and Docker",
    "version": "0.1.0",
    "readme": "README.md",
    "scripts": {
        "dev": "nodemon --watch 'src/**/*.ts' --exec node --inspect=0.0.0.0:9229 -r ts-node/register src/index.ts",
        "build": "tsc",
        "start": "node ./dist/index.js",
        "test": "node ./debug/index.mjs",
        "clean-install": "rimraf node_modules && npm install",
        "docker:dev": "docker exec safari_fastify_server npm run dev",
        "docker:test": "docker exec safari_fastify_server npm run test",
        "docker:init:bash": "bash ./scripts/docker_init.sh",
        "docker:init:batch": ".\\scripts\\docker_init.bat",
        "scripts:pswd:generate": "node ./debug/scripts/generatePassword.mjs"
    },
    "dependencies": {
        "@fastify/cookie": "latest",
        "@fastify/rate-limit": "latest",
        "@fastify/redis": "latest",
        "@fastify/swagger": "latest",
        "@fastify/swagger-ui": "latest",
        "@prisma/client": "latest",
        "bcryptjs": "latest",
        "fastify": "latest",
        "fastify-plugin": "latest",
        "jsonwebtoken": "latest",
        "qs": "latest"
    },
    "devDependencies": {
        "@types/bcryptjs": "latest",
        "@types/jsonwebtoken": "latest",
        "@types/node": "latest",
        "@types/qs": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint": "latest",
        "eslint-config-prettier": "latest",
        "eslint-plugin-prefer-arrow": "latest",
        "nodemon": "latest",
        "prettier": "latest",
        "prisma": "latest",
        "ts-node": "latest",
        "tsconfig-paths": "latest",
        "typescript": "latest",
        "undici": "latest"
    }
}